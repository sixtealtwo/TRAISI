{
	"compilerOptions": {
		// "typeRoots": ["../../node_modules/@types"],
		"allowSyntheticDefaultImports": true,
		"lib": ["es2017", "dom", "esnext"],
		"rootDirs": ["../shared/services"],
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": false,
		"moduleResolution": "node",
		"outDir": "../out-tsc/app",
		"baseUrl": "./",
		"target": "es2015",
		"module": "es6",
		"types": ["node", "jquery", "jasmine","google.analytics"],
		"paths": {
			"app/*": ["app/*"],
			"shared/*": ["../shared/*"],
			"rxjs": ["../node_modules/rxjs"],
			"rxjs/*": ["../node_modules/rxjs/*"]
		},
		"resolveJsonModule": true
	},
	"files": ["polyfills.ts"],
	"include": ["./**/*.ts", "../shared/*.ts"],
	"exclude": ["src/test.ts", "../admin-app/**/*", "**/*.spec.ts"]
}
